<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="130.0" prefWidth="176.0" styleClass="mainElement" stylesheets="@app-view.css"
      xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="egs.gui.PopupController">
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
   </padding>
   <VBox alignment="CENTER" prefHeight="70.0" prefWidth="155.0" styleClass="borderBox" VBox.vgrow="ALWAYS">
      <Label fx:id="infoLabel" alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308"
             prefWidth="220.0" styleClass="labelSimple" text="Komunikat" textAlignment="CENTER" VBox.vgrow="ALWAYS">
         <font>
            <Font size="14.0"/>
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#onOkClicked" text="Ok">
         <VBox.margin>
            <Insets bottom="10.0"/>
         </VBox.margin>
      </Button>
   </VBox>
</VBox>
